class Rectangle {
    double length, width;
    void input(double a,double b) {
        length=a; width=b;}
    double calculateArea(){
              double area=length*width;
               return area;}
    void display(){
        System.out.println(length);System.out.println(width);}}
class Main {
    public static void main(String[] args) {
        Rectangle r = new Rectangle();
        r.input(300,400);
        r.display();
        double t=r.calculateArea();
        System.out.println(t);
   }}
