class Rectangle {
    double leng, wid;
    void input(double a,double b) {
        leng=a; wid=b;}
    double calculateArea(){
              double area=leng*wid;
               return area;}
    void display(){
        System.out.println(leng);System.out.println(wid);}}
class Main {
    public static void main(String[] args) {
        Rectangle rec = new Rectangle();
        rec.input(300,400);
        rec.display();
        double t=rec.calculateArea();
        System.out.println(t);
   }}
